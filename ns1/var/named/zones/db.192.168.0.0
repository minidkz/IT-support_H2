$TTL 86400
@ IN SOA ns1.nisseland.local. nisseland.local. (
  2020080302 ;Serial
  7200 ;Refresh
  3600 ;Retry
  1209600 ;Expire
  3600 ;Minimum TTL
)

;Name Server Infomation
@ IN NS ns1.nisseland.local.

;IP address of Name Server
ns1 IN  A 192.168.0.6
ns2 IN  A 192.168.0.7

;PTR Record IP address to Hostname
1 IN  PTR router
2 IN  PTR jump
3 IN  PTR storage
4 IN  PTR www
5 IN  PTR db
6 IN  PTR ns1
7 IN  PTR ns2
